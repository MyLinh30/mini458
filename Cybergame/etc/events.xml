<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="checkout_cart_product_add_after">
        <observer name="checkout_cart_product_add_after_observer" instance="Magenest\Cybergame\Observer\CheckoutCartAddObserver" disabled="true"/>
    </event>
    <event name="sales_model_service_quote_submit_before">
        <observer name="sales_model_service_quote_submit_before_observer" instance="Magenest\Cybergame\Observer\QuoteSubmitObserver" disabled="true"/>
    </event>

    <event name="checkout_submit_all_after">
        <observer name="checkout_submit_all_after_observer" instance="Magenest\Cybergame\Observer\HandleAccount"/>
    </event>
</config>
