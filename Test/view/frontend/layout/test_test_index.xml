<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account_index.xml"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Show Test Address</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magenest\Test\Block\Test\MyTestAddress" name="my_test_address"  template="Magenest_Test::account/test/my_test_address.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
